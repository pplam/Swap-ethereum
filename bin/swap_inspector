#!/usr/bin/env node

const Web3 = require('web3')
const abi = require('./abi/Swap.json')

const endpoint = 'http://localhost:2000'

const provider = new Web3.providers.HttpProvider(endpoint)
const web3 = new Web3(provider)

const SWAP = '0xfa3410abeaf33cd4514eb18250e15d37d64be32a'

const gas = 470000
const from = '0x30480776de6664e03763ea79bcc340c527300555'

const swap = new web3.eth.Contract(abi, SWAP, {from, gas})

swap.methods.outTxs(parseInt(process.argv[2])).call().then(console.log)
